<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.internetbankingfrontend.controller.savings.WithdrawSavingsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Enter the sum you want to withdraw from the savings account"/>
    <TextField fx:id="withdrawSavingsSumField" promptText="Enter sum of money"/>
    <Button text="Withdraw" onAction="#withdrawSavingSum"/>
</VBox>