<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.internetbankingfrontend.controller.customer.AddCustomerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Customer Username"/>
    <TextField fx:id="CustomerUsername" promptText="Enter Username"/>
    <TextField fx:id="CustomerPassword" promptText="Enter Password"/>
    <TextField fx:id="CustomerFirstName" promptText="Enter First Name"/>
    <TextField fx:id="CustomerLastName" promptText="Enter Last Name"/>
    <TextField fx:id="CustomerPhoneNumber" promptText="Enter Phone Number"/>
    <Button text="Add Customer" onAction="#AddCustomer"/>
</VBox>
