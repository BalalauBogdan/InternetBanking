<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.internetbankingfrontend.controller.User.UserController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="User"/>
    <Button text="Sold" onAction="#openShowSold"/>
    <Button text="Deposit" onAction="#openDepositMoney"/>
    <Button text="Withdraw" onAction="#openWithdrawMoney"/>
    <Button text="Transfer" onAction="#openTransferMoney"/>
    <Button text="Pending Transactions" onAction="#openPendingTransaction"/>
    <Button text="Transactions History" onAction="#openTransactionsHistory"/>
    <Button text="Savings Account" onAction="#openSavingMenu"/>
</VBox>
